{
  "name": "portfolio-react",
  "version": "1.0.0",
  "description": "Portfolio moderne avec React",
  "type": "module",
  "scripts": {
    "dev": "vite --config frontend/vite.config.js",
    "server": "node backend/index.js",
    "dev:all": "concurrently \"npm:server\" \"npm:dev\"",
    "build": "vite build --config frontend/vite.config.js",
    "build:vercel": "cd frontend && npm run build",
    "preview": "vite preview --config frontend/vite.config.js"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "framer-motion": "12.23.16",
    "lucide-react": "0.544.0",
    "next": "15.5.3",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-icons": "^5.5.0",
    "resend": "3.5.0",
    "tailwind-merge": "3.3.1",
    "tailwindcss-animate": "1.0.7"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.0.0",
    "concurrently": "8.2.2",
    "postcss": "^8.5.6",
    "vite": "^4.4.0"
  }
